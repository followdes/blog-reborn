<div class="container text-center">
    <%= will_paginate @users%>
    <% @users.each do |user|%>
  <h1><%= link_to  user.username , user_path(user)%> </h1>
  <% if logged_in? && current_user.admin? %>
      <%= link_to "delete user", user_path(user), method: :delete %>
  <% end %>
  <% end %>
  <%= will_paginate @users %>
  </div>